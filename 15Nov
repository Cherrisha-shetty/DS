#include <stdio.h>
#include <stdlib.h>
#define MAX 50
int queue_array[MAX];
int rear=0;
int front=0;

void insert()
{
if(rear=MAX)
printf("\nQueue is full.");
else
{
printf("\n Insert an element");
scanf("%d", &queue_array[rear]);
rear=rear+1;
printf("\n Element has been inserted");
}
}
void delete()
{
if(front== rear)
printf("\n Queue is empty.");
else
{
printf("\n Deleted item is:%d\n", queue_array[front]);
front=front+1;
}
}

void display()
{
int i;
if (front==0 || rear == 0)
printf("\n Queue is empty \n");
else
{
printf("\n Queue is:");
for(i=0; i<rear; i++)
printf("%d/t", queue_array[i]);
}
}

int main()
{
int choice;
while(1)
{
printf("\n 1. Insert \n");
prinft("\n 2. Delete \n");
printf("\n 3. Display \n");
printf("\n 4. Exit \n");
printf("\n Enter your choice");
scanf("%d",&choice);
switch(choice)
{
case 1:
insert();
break;
case 2:
delete();
break;
case 3:
display();
break;
case 4:
exit(1);
default:
printf("\n Invalid choice. \n");
}
}
}
